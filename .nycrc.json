{
  "check-coverage": true,
  "per-file": false,
  "watermarks": {
    "lines": [90, 95],
    "functions": [90, 95],
    "branches": [90, 95],
    "statements": [90, 95]
  },
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "require": [
    "ts-node/register"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "test/",
    "stories/",
    "src/ui/**/*.tsx",
    "src/ui/components/**/index.ts",
    "src/simulate-worker/*.ts",
    "src/lib/logger.ts",
    "docs/",
    "coverage/",
    "*.js",
    "**/*.d.ts",
    "**/i-*.ts",
    "**/t-*.ts"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "cache-dir": ".nyc/cache",
  "temp-dir": ".nyc/temp",
  "report-dir": "coverage",
  "cache": true,
  "sourceMap": false,
  "instrument": false,
  "all": true
}
