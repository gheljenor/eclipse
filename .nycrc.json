{
  "check-coverage": true,
  "per-file": false,
  "watermarks": {
    "lines": [90, 95],
    "functions": [90, 95],
    "branches": [90, 95],
    "statements": [90, 95]
  },
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "require": [
    "ts-node/register"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "coverage/",
    "test/",
    "stories/",
    "src/ui/",
    "*.js",
    "**/*.d.ts",
    "**/i-*.ts",
    "**/t-*.ts"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "cache-dir": ".nyc/cache",
  "temp-dir": ".nyc/temp",
  "report-dir": "coverage",
  "cache": true,
  "sourceMap": true,
  "instrument": true,
  "all": true
}
